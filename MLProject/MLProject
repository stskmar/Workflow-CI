entry_points:
  main:
    parameters:
      n_estimators:
        type: int
        default: 200
      max_depth:
        type: int
        default: 15
      data_path:
        type: path
        default: MLProject/bank_clean.csv
    command: >
      python modelling.py
      {n_estimators}
      {max_depth}
      {data_path}
    experiment_name: bank_marketing_rf