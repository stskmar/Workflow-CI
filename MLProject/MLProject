name: bank_marketing_rf

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      n_estimators:
        type: int
        default: 100
      max_depth:
        type: int
        default: 10
      data_path:
        type: path
        default: bank_clean.csv
    command: >
      python modelling.py
      {n_estimators}
      {max_depth}
      {data_path}